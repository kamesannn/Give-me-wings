#:include pipe.kv
#:import gameoverwindow gameoverwindow

FloatLayout:
    name: "game"
    GameOverWindow:
    Background:
        id: background
        canvas.before:
            Rectangle:
                size: self.size
                pos: self.pos
                source: "bg.png"
            Rectangle:
                size: self.width, 96
                pos: self.pos[0], self.pos[1]
                texture: self.floor_texture
    Label:
        id: score
        size_hint_y: None
        height: 96
        text: "0"
        font_size: 32
    Label:
        size_hint: None, None
        pos: 279, 0.5
        text: "Score: "
        font_size: 32
    Character:
        source: "jump1.png"
        size_hint: None, None
        size: 84, 107
        pos: 70, root.height - 250
        id: character
